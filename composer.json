{
    "name": "orchid/fortify",
    "description": "Orchid template for Laravel Fortify",
    "keywords": [
        "orchid",
        "fortify"
    ],
    "homepage": "https://github.com/orchid/fortify",
    "license": "MIT",
    "authors": [
        {
            "name": "Alexandr Chernyaev",
            "email": "bliz48rus@gmail.com",
            "homepage": "http://orchid.software/",
            "role": "Developer"
        }
    ],
    "require": {
        "laravel/fortify": "^1.7",
        "orchid/platform": "^12.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "orchestra/testbench": "^7.0",
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "Orchid\\Fortify\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Orchid\\Fortify\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --colors=always",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Orchid\\Fortify\\AuthServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
